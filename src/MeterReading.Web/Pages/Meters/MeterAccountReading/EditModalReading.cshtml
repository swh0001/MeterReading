@page
@using MeterReading.Localization
@using Microsoft.AspNetCore.Mvc.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal;
@inject IHtmlLocalizer<MeterReadingResource> L
@model MeterReading.Web.Pages.Meters.MeterAccountReading.EditModalReadingModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="ViewModelReading" data-ajaxForm="true" asp-page="EditModalReading">
    <abp-modal>
        <abp-modal-header title="@L["EditMeterAccountReading"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="ViewModelReading.Id" />


            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
